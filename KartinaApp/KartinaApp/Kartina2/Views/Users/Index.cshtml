            @Html.DisplayFor(modelItem => item.MotDePasse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telephone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Biographie)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.id }) |
